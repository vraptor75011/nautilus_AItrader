{
    "venvPath": "/home/ubuntu",
    "venv": "deepseek_venv",
    "pythonVersion": "3.12",
    "pythonPlatform": "Linux",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.12",
            "pythonPlatform": "Linux",
            "extraPaths": [
                "strategy",
                "utils",
                "indicators"
            ]
        }
    ],
    "include": [
        "**/*.py"
    ],
    "exclude": [
        "**/__pycache__",
        "**/node_modules",
        "**/.git",
        "**/logs",
        "**/.*"
    ],
    "reportMissingImports": "warning",
    "reportMissingTypeStubs": false,
    "typeCheckingMode": "basic"
}

